
{% use "@partials/localizacao.html.twig" with localizacao as parent_localizacao %}

{% block tabela_busca %}

	<form name='{{ busca.formulario }}' id='{{ busca.formulario }}' method='{{ busca.method }}' action="">
		{{ busca.input|raw }}

		<div class="panel panel-default">
			<!-- Default panel contents -->

			{% block sidebar %}
				{{ block('parent_localizacao') }}
			{% endblock %}
			{% if busca.campos is not empty %}
				<table class="table table-striped table-bordered" cellspacing="0" width="100%">
					<thead>
						<tr>
							<th>{{ busca.titulo|raw }}</th>
							<th></th>
						</tr>
					</thead>
					<tbody>
						<tr>
							{{ busca.campos|raw }}
						</tr>
						<tr>
							<td colspan="2" align="center">
								{{ busca.botao|raw }}
							</td>
						</tr>
					</tbody>
				</table>
			{% endif %}
		</div>
	</form>

{% endblock %}
